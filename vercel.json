{
  "version": 2,
  "builds": [
    { "src": "vite.config.ts", "use": "@vercel/vite" },
    { "src": "api/send-contact.ts", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/api/send-contact",
      "dest": "api/send-contact.ts"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}